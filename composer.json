{
    "name": "epmnzava/beempayments",
    "description": "A laravel lib that helps you to easily integrate with beem africa payments api.",
    "keywords": [
        "epmnzava",
        "beempayments",
        "mobilepayments"
    ],
    "homepage": "https://github.com/epmnzava/beempayments",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Emmanuel Mnzava",
            "email": "emmanuel@opensource.co.tz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^7.2|^7.3|^7.4|^8.0",
        "epmnzava/bill-me": "^1.0.7",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^5.5.44|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Epmnzava\\Beempayments\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Epmnzava\\Beempayments\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Epmnzava\\Beempayments\\BeempaymentsServiceProvider"
            ],
            "aliases": {
                "Beempayments": "Epmnzava\\Beempayments\\BeempaymentsFacade"
            }
        }
    }
}
